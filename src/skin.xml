		 <action name="hotspot_animate">

		inc(frame,1,get(lastframe),0);

		mul(ypos,frame,frameheight);

		txtadd(crop,'0|',get(ypos),'|',get(framewidth),'|',get(frameheight));

		delayedcall(0.10, if(loaded, hotspot_animate() ) );

	</action>

	

	

