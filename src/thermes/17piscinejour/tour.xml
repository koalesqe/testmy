	</action>
	
	 <events onloadcomplete="action(loader); js(hotelthermes());"/>


	
	<scene name="scene_17piscinejour" nameid="scene_17piscinejour" etablissement="Thermes Marins Monte-Carlo"  dossier="thermes" galeriephoto="oui" galerievideo="non" info="non" title="Swimming Pool - Day View" onstart="" thumburl="panos/17piscinejour.tiles/thumb.jpg" lat="" lng="" heading="">

