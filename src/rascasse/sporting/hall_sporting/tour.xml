<krpano title="Virtual Tour" onstart="startup();" basedir="%HTMLPATH%">


<include url="../../skin.xml" />
    

    <action name="startup">
        loadscene(scene_hall_sporting, null, MERGE, BLEND(1));
		
    </action>
	
	
	<events onloadcomplete="action(loader); js(hotelsporting());"/>
	
	<scene name="scene_hall_sporting" nameid="scene_hall_sporting" etablissement="Sporting Monte-Carlo" dossier="sporting" galeriephoto="oui" galerievideo="non" info="non" title="Lobby" onstart="" thumburl="panos/hall_sporting.tiles/thumb.jpg" lat="" lng="" heading="">

		<view hlookat="123.476" vlookat="0.198" fovtype="MFOV" fov="120.000" maxpixelzoom="1.0" fovmin="70" fovmax="140" limitview="auto" />

		<preview url="panos/hall_sporting.tiles/preview.jpg" />

		<image>
			<cube url="panos/hall_sporting.tiles/pano_%s.jpg" />
			<mobile>
				<cube url="panos/hall_sporting.tiles/mobile_%s.jpg" />
			</mobile>
		</image>

