		<preview url="panos/sporting_out.tiles/preview.jpg" />

		<image>
			<cube url="panos/sporting_out.tiles/pano_%s.jpg" />
			<mobile>
				<cube url="panos/sporting_out.tiles/mobile_%s.jpg" />
			</mobile>
		</image>

		<!-- place your scene hotspots here -->
		<hotspot name="spot1" style="skin_hotspotstyle" ath="-120.261" atv="-24.394"  onclick="sporting_air" tooltip="Sporting Monte-Carlo - Aerial View"/>
		<hotspot name="spot2" style="skin_hotspotstyle" ath="-3.513" atv="-2.429"  onclick="sporting_hall" tooltip="Lobby"/>

	</scene>


</krpano>
