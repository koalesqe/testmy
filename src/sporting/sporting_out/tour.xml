<krpano version="1.17.1" title="Virtual Tour" onstart="startup();" basedir="%HTMLPATH%"><include url="../../skin.xml" /> 
 <action name="startup">

        loadscene(scene_sporting_out, null, MERGE, BLEND(1));

    </action>




 <events onloadcomplete="action(loader); js(hotelsporting());"/>

	
	<scene name="scene_sporting_out" nameid="scene_sporting_out" etablissement="Sporting Monte-Carlo" dossier="sporting" galeriephoto="oui" galerievideo="non" info="non" title="Entrance" onstart="" thumburl="panos/sporting_out.tiles/thumb.jpg" lat="" lng="" heading="">

